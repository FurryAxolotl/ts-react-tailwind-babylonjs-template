{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"dom",
			"es6"
		],
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"noResolve": false,
		"isolatedModules": true,
		"removeComments": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"jsx": "react-jsx"
	},
	"include": ["src"]
}